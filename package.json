{
  "name": "dapps-template",
  "version": "0.0.0",
  "main": "./dist/server.js",
  "engines": {
    "node": ">=8.12.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "flow": "flow",
    "eslint": "eslint .",
    "webpack-dev": "webpack --config config/webpack/client/webpack.config.dev.js --config config/webpack/server/webpack.config.dev.js",
    "webpack-prod": "webpack --config config/webpack/client/webpack.config.prod.js --config config/webpack/server/webpack.config.prod.js",
    "dev": "concurrently \"webpack -w --config config/webpack/server/webpack.config.dev.js\" \"webpack -w --config config/webpack/client/webpack.config.dev.js\" \"nodemon --watch ./dist ./dist/server.js\"",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"**/*.{js,jsx,json,md}\"",
    "test": "./node_modules/jest/bin/jest.js"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "~3.2.2",
    "@material-ui/icons": "~3.0.1",
    "config": "~1.30.0",
    "cookie-parser": "~1.4.3",
    "classnames": "~2.2.6",
    "debug": "~2.6.9",
    "deepmerge": "~2.1.1",
    "dotenv": "~6.1.0",
    "express": "~4.16.0",
    "flow-typed": "~2.5.1",
    "history": "~4.7.2",
    "http-errors": "~1.6.2",
    "immutable": "~3.8.2",
    "jade": "~1.11.0",
    "joi": "~13.6.0",
    "jsonwebtoken": "~8.3.0",
    "morgan": "~1.9.0",
    "node-fetch": "~2.2.0",
    "query-string": "~6.1.0",
    "react": "~16.6.0",
    "react-dom": "~16.6.0",
    "react-redux": "~5.0.7",
    "react-router": "~4.3.1",
    "react-router-dom": "~4.3.1",
    "react-router-redux": "~4.0.8",
    "redux": "~4.0.0",
    "redux-thunk": "~2.3.0",
    "safe-json-stringify": "~1.2.0",
    "web3": "~1.0.0-beta.36",
    "winston": "~3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "circular-dependency-plugin": "^5.0.2",
    "concurrently": "^3.6.0",
    "config-webpack": "^1.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "flow-babel-webpack-plugin": "^1.1.1",
    "flow-bin": "^0.74.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jade-loader": "^0.8.0",
    "jest": "^23.6.0",
    "jest-extended": "^0.11.0",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.2",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.5",
    "react-test-renderer": "^16.5.2",
    "superagent": "^4.0.0-beta.5",
    "supertest": "^3.3.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
